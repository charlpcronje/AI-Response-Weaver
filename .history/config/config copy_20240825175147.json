      "remove_first_line": false
    },
    "script": {
      "extensions": [
        ".sh",
        ".bash",
        ".zsh",
        ".ksh"
      ],
      "regex": "^\\s*(?:#)\\s*([\\w.-]+(?:/[\\w.-]+)*\\.[\\w]+)\\s*$",
