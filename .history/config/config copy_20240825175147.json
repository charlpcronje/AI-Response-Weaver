{
  "file_types": {
    "web": {
      "extensions": [
        ".html",
        ".htm",
        ".css",
        ".scss",
        ".sass",
        ".less",
        ".styl",
        ".js",
        ".jsx",
        ".ts",
        ".tsx",
        ".vue",
        ".svelte",
        ".pug",
        ".jade",
        ".ejs",
        ".haml",
        ".twig",
        ".njk",
        ".marko",
        ".mustache",
        ".handlebars",
        ".dust",
        ".wxml",
        ".wxss"
      ],
      "regex": "^\\s*(?://|#|<!--)\\s*([\\w.-]+(?:/[\\w.-]+)*\\.[\\w]+)\\s*(?:-->)?$",
      "remove_first_line": false
    },
    "data": {
      "extensions": [
        ".json",
        ".json5",
        ".yaml",
        ".yml",
        ".xml",
        ".ini",
        ".env",
        ".toml"
      ],
      "regex": "^\\s*(?://|#|<!--)\\s*([\\w.-]+(?:/[\\w.-]+)*\\.[\\w]+)\\s*(?:-->)?$",
      "remove_first_line": false
    },
    "config": {
      "extensions": [
        ".editorconfig",
        ".prettierrc",
        ".stylelintrc",
        ".eslintrc",
        ".babelrc",
        ".tsconfig.json",
        ".jsconfig.json",
        ".csslintrc",
